{
  "name": "api_express_typescript_hello-world",
  "version": "1.0.1",
  "description": "Auth0 Express.js API code sample with TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node-dev --require dotenv/config --respawn --pretty --transpile-only src/index.ts",
    "build": "tsc && npm run copy-files",
    "copy-files": "mkdir -p .aws-sam/build/TaskVisionFunction && cp package.json .aws-sam/build/TaskVisionFunction/ && cp -r src .aws-sam/build/TaskVisionFunction/ && cd .aws-sam/build/TaskVisionFunction && npm install --production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sam:build": "npm run build && sam build",
    "sam:deploy": "sam deploy --guided",
    "sam:local": "sam local start-api",
    "create-table": "ts-node scripts/create-table.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.826.0",
    "@aws-sdk/lib-dynamodb": "^3.826.0",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^7.4.4",
    "express": "^4.21.2",
    "express-jwt": "^8.4.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "helmet": "^5.1.1",
    "jwks-rsa": "^3.1.0",
    "nocache": "^3.0.4",
    "openai": "^4.67.1",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ulid": "^3.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "prettier": "^2.7.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
