# This is a sample SAM configuration file for local development.
# Copy it to `.samconfig.dev.toml` and fill in your actual values.
# Do NOT commit `.samconfig.dev.toml` to source control.

version = 0.1

[dev.deploy.parameters]
stack_name = "your-dev-stack-name"
resolve_s3 = true
s3_prefix = "your-dev-s3-prefix"
region = "your-aws-region"
capabilities = "CAPABILITY_IAM"
parameter_overrides = "Environment=\"dev\" ClientOriginUrl=\"https://taskvision.ai\" TableName=\"your-dev-table-name\" CustomDomainName=\"api.taskvision.ai\" HostedZoneId=\"your-hosted-zone-id\""
